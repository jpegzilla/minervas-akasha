{
  "name": "minervas_akasha",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@types/react-router-dom": "^5.1.7",
    "bcryptjs": "^2.4.3",
    "eslint": "^7.20.0",
    "exif-js": "^2.3.0",
    "jsmediatags": "^3.9.5",
    "music-metadata-browser": "^2.2.5",
    "node-sass": "^7.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "style:watch:min": "sass --watch --style=compressed ./src/components/styles/main.sass:./src/components/styles/main.min.css",
    "swm": "sass --watch --style=compressed ./src/components/styles/main.sass:./src/components/styles/main.min.css",
    "style:watch": "sass --watch --style=expanded ./src/components/styles/main.sass:./src/components/styles/main.css",
    "sw": "sass --watch --style=expanded ./src/components/styles/main.sass:./src/components/styles/main.css",
    "formatall": "prettier --write ./src/**/**/**/**/**/**/*.js",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test:unit": "mocha --require @babel/register --require ./test/helpers/helpers.js --require ./test/helpers/dom.js --require ignore-styles -b -c './test/*.test.js'",
    "test:util": "mocha --require @babel/register --require ./test/helpers/helpers.js --require ./test/helpers/dom.js --require ignore-styles -b -c './test/utility-tests/*.test.js'",
    "test:watch": "npm run test -- --watch",
    "test:util:watch": "npm run test:util -- --watch",
    "eject": "react-scripts eject",
    "lint": "eslint --config node_modules/eslint-config-react-app/index.js ./src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.3",
    "chai": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "es6-promise": "^4.2.8",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jsdom": "^16.4.0",
    "mocha": "^8.3.0",
    "react-addons-test-utils": "^15.6.2",
    "react-app-rewired": "^2.1.8",
    "worker-loader": "^3.0.8"
  }
}
